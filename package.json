{
  "name": "web-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "clean": "rm -rf dist/",
    "package": "npm install && ng build --prod && cp server/*.js* dist/web-ui && cd dist  && zip -r -D web-ui.zip web-ui/*",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
